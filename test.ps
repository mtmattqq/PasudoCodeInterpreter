Algorithm max(x, y):
    if x > y then x else y

n <- int(read())
t <- int(read())

a <- for i <- 1 to n do 0
dp <- for i <- 1 to n do 0

for i <- 1 to n do
    a[i] <- int(read())

dp[1] <- a[1]
if a[1] > t then
    dp[2] <- max(a[1], a[2])
else
    dp[2] <- a[1] + a[2]

for i <- 3 to n do
    if a[i - 1] > t then
        dp[i] <- max(dp[i - 1], dp[i - 2] + a[i])
    else
        dp[i] <- dp[i - 1] + a[i]
print(max(dp[n], dp[n - 1]))